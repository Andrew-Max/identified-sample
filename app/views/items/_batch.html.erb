<h3>Batch <%= batch.id %></h3>

<input type="button" onclick="tableToExcel('batch-<%= batch.id %>', 'newfile')" value="Export to Excel">
<table id="batch-<%= batch.id %>"class="clearance-batch">

  <%= render "tables/item_header" %>
  <tbody>
    <%= render partial: "items/item", collection: batch.items %>
  </tbody>

  <%= render "tables/batch_statistics", batch: batch %>
</table>
