<%= form_tag(clearance_batches_path, multipart: true, class: 'form-inline', role: 'form') do %>
  <div class="form-group">
    <%= hidden_field_tag "csv_file", true %>
    <%= file_field_tag 'csv_batch_file', class: 'form-control'%>
  </div>
  <%= submit_tag 'upload batch file', class: 'btn btn-primary', data: { disable_with: "Submitting..." } %>
<% end %>

<a href="/?loose=true" class="switch btn btn-success" >Switch to Id Input</a>
