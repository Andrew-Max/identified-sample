<!DOCTYPE html>
<html>
<head>
  <title>TakeHomeChallenge</title>
  <%= stylesheet_link_tag 'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>

  <!-- data tables cdn -->
  <script type="text/javascript" src="http://cdn.datatables.net/1.10.7/js/jquery.dataTables.min.js"></script>
  <link rel="stylesheet" type="text/css" href="http://cdn.datatables.net/1.10.7/css/jquery.dataTables.min.css">
  <%= csrf_meta_tags %>

</head>
<body>

  <div class="container <%= controller_name %> <%= action_name %>">
    <%= render "layouts/header" %>
    <%= render "layouts/flash_notices" %>
    <%= yield %>
  </div>

</body>
</html>
